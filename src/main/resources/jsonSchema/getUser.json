{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "description": "GET /user/{username}",
  "properties": {
    "id": {
      "type": "integer"
    },
    "username": {
      "type": "string"
    },
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "password": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "userStatus": {
      "type": "integer"
    }
  },
  "required": [
    "id",
    "username",
    "email",
    "password"
  ]
}
